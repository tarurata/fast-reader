{
  "manifest_version": 3,
  "name": "FastReader",
  "version": "1.0",
  "description": "A speed reading extension for Chrome",
  "icons": {
    "128": "icon.png"
  },
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}